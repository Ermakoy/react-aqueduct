{
  "name": "react-aqueduct",
  "version": "0.8.4",
  "files": [
    "lib"
  ],
  "main": "lib/react-aqueduct.js",
  "author": "Sergey Korchinskiy <k.serjey@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kserjey/react-aqueduct.git"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint src",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "test": "jest",
    "build": "rollup -c",
    "prepare": "npm run build"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.test.js"
    ],
    "testURL": "http://localhost"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.55",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-react": "^7.0.0-beta.55",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.4.2",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0",
    "prettier-eslint": "^8.8.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-testing-library": "^4.1.5",
    "rollup": "^0.63.5",
    "rollup-plugin-babel": "^4.0.0-beta.7",
    "rollup-plugin-size-snapshot": "^0.6.1"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.0.1",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.2"
  }
}
